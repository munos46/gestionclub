<?xml version="1.0" encoding="UTF-8"?>
<manifest
    package="com.adoliveira.gestionclub"
    xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-feature
        android:name="android.hardware.camera" android:required="false"/>
    <uses-feature
        android:name="android.hardware.camera.autofocus" android:required="false"/>
    <uses-feature
        android:name="android.hardware.camera.front" android:required="false"/>
    <uses-feature
        android:name="android.hardware.camera.back" android:required="false"/>
    <application
        android:allowBackup="false"
        android:icon="@drawable/logo_lancement"
        android:label="@string/app_name"
        android:name="CustomApplication"
        android:theme="@style/AppTheme" tools:replace="icon, android:allowBackup">
        <activity
            android:label="@string/activityname_resetsettingsandexitdialog"
            android:name="com.adeuza.movalysfwk.mobile.mf4android.activity.ResetSettingsAndExitDialogActivity" android:theme="@style/Theme.Dialog"/>
        <activity
            android:label="@string/activityname_parameter" android:name="com.adeuza.movalysfwk.mobile.mf4android.activity.ParameterActivity">
            <intent-filter>
                <action android:name="com.adeuza.movalysfwk.mobile.mf4android.activity.business.displaysettings.ACTION"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:label="DBImport"
            android:name="com.adeuza.movalysfwk.mobile.mf4android.activity.DBImportValidationActivity" android:theme="@style/Theme.Dialog"/>
        <activity android:name="com.adeuza.movalysfwk.mobile.mf4android.ui.command.MMPhotoCommand"/>
        <!--//@generated-start[gen-activities][X]-->
        <activity
            android:label="@string/DetailEntrainScreen"
            android:name="com.application.DetailEntrainScreen"
            android:windowSoftInputMode="adjustPan" xmlns:android="http://schemas.android.com/apk/res/android"/>
        <activity
            android:label="@string/DetailJoueurScreen"
            android:name="com.application.DetailJoueurScreen"
            android:windowSoftInputMode="adjustPan" xmlns:android="http://schemas.android.com/apk/res/android"/>
        <activity
            android:label="@string/ListEntrainScreen"
            android:name="com.application.ListEntrainScreen"
            android:windowSoftInputMode="adjustPan" xmlns:android="http://schemas.android.com/apk/res/android"/>
        <activity
            android:label="@string/ListJoueurScreen"
            android:name="com.application.ListJoueurScreen"
            android:windowSoftInputMode="adjustPan" xmlns:android="http://schemas.android.com/apk/res/android"/>
        <activity
            android:label="@string/app_name"
            android:name="com.application.Accueil"
            android:windowSoftInputMode="adjustPan" xmlns:android="http://schemas.android.com/apk/res/android">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <!--//@generated-end-->
    </application>
</manifest>
