<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout
    android:id="@+id/glistentrainpanel__listitem1__master"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mdk="http://schemas.android.com/apk/res-auto" xmlns:movalys="http://www.adeuza.com/movalys/mm/android">
    <android.support.v7.widget.CardView
        android:layout_height="match_parent"
        android:layout_marginBottom="6dp"
        android:layout_marginEnd="@dimen/activity_vertical_padding"
        android:layout_marginStart="@dimen/activity_vertical_padding"
        android:layout_marginTop="6dp"
        android:layout_width="match_parent">
        <LinearLayout
            android:layout_height="match_parent"
            android:layout_width="match_parent" android:orientation="vertical">
            <GridLayout android:columnCount="2"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/list_padding_bottom"
                android:paddingLeft="@dimen/list_padding_top"
                android:paddingRight="@dimen/list_padding_top"
                android:paddingTop="@dimen/list_padding_top"
                android:rowCount="2">

                <com.adeuza.movalysfwk.mobile.mf4android.ui.views.MMDateTextView
                    android:focusable="false"
                    android:id="@+id/vmlistentrainpanelimpl__dateHeure__value"
                    android:layout_column="0"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                    android:textColor="@color/textColorPrimary"
                    mdk:hint="@string/vmlistentrainpanelimpl__dateHeure__label"
                    mdk:mandatory="true"
                    mdk:readOnly="true"
                    movalys:date_format="short" movalys:format="E d MMM yyyy"/>

                <com.adeuza.movalysfwk.mobile.mf4android.ui.views.MMDateTextView
                    android:focusable="false"
                    android:id="@+id/vmlistentrainpanelimpl__heure__value"
                    android:layout_column="1"
                    android:layout_gravity="right"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                    android:textColor="@color/textColorPrimary"
                    mdk:mandatory="true"
                    mdk:readOnly="true"
                    movalys:date_format="short" movalys:format="HH:mm"/>
                <TextView android:focusable="false"
                    android:id="@+id/vmlistentrainpanelimpl__nom__value"
                    android:layout_columnSpan="2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:maxLength="255"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body1" android:textColor="@color/textColorSecondary"/>
            </GridLayout>
        </LinearLayout>
    </android.support.v7.widget.CardView>
</RelativeLayout>
