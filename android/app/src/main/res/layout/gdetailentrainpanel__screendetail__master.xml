<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout
    android:id="@+id/gdetailentrainpanel__screendetail__master"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android" xmlns:mdk="http://schemas.android.com/apk/res-auto">
    <LinearLayout
        android:id="@+id/gdetailentrainpanel__screendetail__visualpanel"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/activity_vertical_padding"
            android:layout_marginRight="@dimen/activity_vertical_padding"
            android:layout_marginTop="@dimen/activity_vertical_padding"
            android:layout_marginBottom="@dimen/activity_vertical_padding">
            <LinearLayout
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal"
                android:layout_marginLeft="@dimen/activity_vertical_padding">
                <!--<com.soprasteria.movalysmdk.widget.basic.MDKRichDateTime
                    android:id="@+id/detail__dateHeure__edit"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:textColor="@color/textColorPrimary"
                    mdk:hint="@string/detail__dateHeure__label"
                    mdk:mandatory="true" mdkLabelStyle="@style/GestionEquipe.Label"/>-->
                <GridLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:columnCount="2"
                    android:rowCount="2"
                    android:layout_weight="0.5">
                    <TextView
                        android:id="@+id/dateCalendarDay"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="27"
                        android:textSize="38dp"
                        android:layout_column="0"
                        android:layout_row="0"
                        android:layout_gravity="center_horizontal"/>
                    <TextView
                        android:id="@+id/dateCalendarMonth"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="janv"
                        android:textSize="14dp"
                        android:layout_column="0"
                        android:layout_row="1"
                        android:layout_marginTop="-10dp"
                        android:layout_gravity="center_horizontal"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="18h30"
                        android:layout_column="1"
                        android:textSize="20dp"
                        android:layout_row="0"
                        android:layout_rowSpan="2"
                        android:layout_gravity="center"/>
                </GridLayout>
                <com.soprasteria.movalysmdk.widget.spinner.MDKRichSpinner
                    android:id="@+id/detail__VMDetailEntrainPanelLieu__edit"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    mdk:hasBlankRow="true"
                    mdk:hint="@string/detail__VMDetailEntrainPanelLieu__label"
                    mdk:mandatory="true"
                    mdkLabelStyle="@style/GestionEquipe.Label"
                    android:layout_weight="0.5" />
            </LinearLayout>
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/activity_vertical_padding"
            android:layout_marginRight="@dimen/activity_vertical_padding"
            android:layout_marginTop="@dimen/activity_vertical_padding"
            android:layout_marginBottom="@dimen/activity_vertical_padding">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/nbPerPresente"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"
                    android:textColor="@color/textColorPrimary"
                    android:layout_marginTop="@dimen/activity_vertical_padding"
                    android:layout_marginBottom="@dimen/activity_vertical_padding"/>
                <View
                    android:background="@color/material_grey200"
                    android:layout_height="1dp" android:layout_width="wrap_content"/>
                <ListView
                    android:divider="@null"
                    android:dividerHeight="0dp"
                    android:id="@+id/detailListJoueur"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_vertical_padding"
                    android:layout_weight="1" android:layout_width="match_parent"/>
            </LinearLayout>
        </android.support.v7.widget.CardView>
        <!--       <com.adeuza.movalysfwk.mobile.mf4android.ui.views.MMActionImageButton
                android:id="@+id/button_SaveDetailEntrainPanel"
                android:layout_below="@id/detail__VMDetailEntrainPanelLieu__edit"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:src="@drawable/ic_btn_ok" movalys:action="com.action.savedetailentrainpanel.SaveDetailEntrainPanel"/>

            <com.adeuza.movalysfwk.mobile.mf4android.ui.views.MMActionImageButton
                android:id="@+id/button_DeleteDetailEntrainPanel"
                android:layout_below="@id/detail__VMDetailEntrainPanelLieu__edit"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/button_SaveDetailEntrainPanel"
                android:layout_width="wrap_content"
                android:src="@drawable/ic_btn_delete" movalys:action="com.action.deletedetailentrainpanel.DeleteDetailEntrainPanel"/> -->
    </LinearLayout>
</RelativeLayout>
