CREATE TABLE IF NOT EXISTS T_JOUEUR(	ID INTEGER NOT NULL,	NOM TEXT NOT NULL,	PRENOM TEXT NOT NULL,	DATENAISSANCE NUMERIC NOT NULL,	PHOTO_NAME TEXT,	PHOTO_URI TEXT,	PHOTO_SVG TEXT,	PHOTO_DATE NUMERIC,	PHOTO_DESC TEXT,	PHOTO_PHOTOSTATE NUMBER(10,0),	EMAIL TEXT,	TAILLE REAL,	POIDS REAL,	ADRESSE TEXT,	COMMUNE TEXT,	VILLE TEXT,	CONSTRAINT PK_JOUEUR PRIMARY KEY (ID));CREATE TABLE IF NOT EXISTS T_LIEU(	ID INTEGER NOT NULL,	NOM TEXT NOT NULL,	POSITION_LATITUDE REAL,	POSITION_LONGITUDE REAL,	POSITION_COMPL TEXT,	POSITION_STREET TEXT,	POSITION_CITY TEXT,	POSITION_COUNTRY TEXT,	CONSTRAINT PK_LIEU PRIMARY KEY (ID));CREATE TABLE IF NOT EXISTS T_ENTRAINEMENT(	ID INTEGER NOT NULL,	DATEHEURE NUMERIC NOT NULL,	LIEUID INTEGER NOT NULL,	CONSTRAINT PK_ENTRAINEMENT PRIMARY KEY (ID),	CONSTRAINT FK_LIEU_ID FOREIGN KEY(LIEUID) REFERENCES T_LIEU(ID) ON UPDATE CASCADE);CREATE TABLE IF NOT EXISTS T_ENTRAINJOUEUR(	JOUEURID INTEGER NOT NULL,	ENTRAINEMENTID INTEGER NOT NULL,	CONSTRAINT PK_ENTRAINJOUEUR PRIMARY KEY (JOUEURID,ENTRAINEMENTID),	CONSTRAINT FK_ENTRAINEMENTS_ID FOREIGN KEY(ENTRAINEMENTID) REFERENCES T_ENTRAINEMENT(ID) ON UPDATE CASCADE ON DELETE CASCADE,	CONSTRAINT FK_JOUEURS_ID FOREIGN KEY(JOUEURID) REFERENCES T_JOUEUR(ID) ON UPDATE CASCADE ON DELETE CASCADE);CREATE INDEX  IF NOT EXISTS I_ENTRAINEMENT_LIEU ON T_ENTRAINEMENT (LIEUID);CREATE INDEX  IF NOT EXISTS I_ENTRAINEMEN_ENTRAINEMEN ON T_ENTRAINJOUEUR (ENTRAINEMENTID);